{% extends 'base.html.twig '%}

{% block title %} Owner details {% endblock %}

{% block body %}

	<div>
	<h2>Pet owner information</h2>
	</div>
	<p>
		<strong>Name :</strong> {{ owner.name  ~ "    |    "~  owner.surname }}		
	</p>
	<table>
	<tr>
		<th>name</th>
		<th>Surname</th>
		<th>Telephone</th>
		<th>Address</th>
		<th>Email</th>
		<!--<th>action</th>-->
