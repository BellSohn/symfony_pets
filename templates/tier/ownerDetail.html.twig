	<tr>
		<td>{{ owner.name }}</td>
		<td>{{ owner.surname }}</td>
		<td>{{ owner.telephone }}</td>
		<td>{{ owner.address }}</td>
		<td>{{ owner.email }}</td>
		<!--<td><a href="#">Editar</a></td>-->

	</tr>
	</table>
	


{% endblock %}